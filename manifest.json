{
    "manifest_version": 3,
    "name": "AB Price Over Spot",
    "version": "1.0",
    "description": "Displays as a percentage the price over spot being charged on all Ainslie Bullion items.",
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "host_permissions": [
        "https://ainsliebullion.com.au/",
        "https://ainsliebullion.com.au/Buy/*"
    ],
    "content_scripts": [
        {
            "js": ["content.js"]
        }
    ]
}
